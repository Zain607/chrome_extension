{
    "manifest_version": 3,
    "name": "LinkedIn Scraper",
    "version": "1.0.0",
    "description": "A basic example extension with only required keys",
    "permissions": [
      "scripting"
    ],
    "background": {
      "service_worker": "service-worker.js",
      "type": "module"
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "https://*/*"
    ]
  }