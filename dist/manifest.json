{
    "manifest_version": 3,
    "name": "LinkedIn Scraper",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*"
    ],
    "icons": {
        "16": "icon32.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}